{
  "name": "KeySpring",
  "version": "1.0.1",
  "manifest_version": 2,
  "description": "Helps Springeteers work",
  "icons": { "128": "icon.png" },
  "background": {
    "persistent": false,
    "scripts": ["keyspring.js"]
  },
  "permissions": [
    "activeTab",
    "https://secure.helpscout.net/"
    ],
  "commands": {
    "call": {
      "suggested_key": {
        "default": "Ctrl+C",
        "mac": "MacCtrl+C"
      },
      "description": "Call person on helpscout"
    }
  }
}
