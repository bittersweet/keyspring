{
  "name": "KeySpring",
  "version": "1.1.0",
  "manifest_version": 2,
  "description": "Helps Springeteers work",
  "icons": { "128": "icon.png" },
  "background": {
    "persistent": false,
    "scripts": ["keyspring.js"]
  },
  "permissions": [
    "activeTab",
    "https://secure.helpscout.net/"
    ],
  "commands": {
    "call": {
      "suggested_key": {
        "default": "Ctrl+C",
        "mac": "MacCtrl+C"
      },
      "description": "Call person on helpscout"
    },
    "newTicket": {
      "suggested_key": {
        "default": "Ctrl+T",
        "mac": "MacCtrl+T"
      },
      "description": "Open new ticket in helpscout"
    },
    "selectPhone": {
      "suggested_key": {
        "default": "Ctrl+P",
        "mac": "MacCtrl+P"
      },
      "description": "Open phone tab on ticket"
    },
    "phoneAndAddTags": {
      "suggested_key": {
        "default": "Ctrl+I",
        "mac": "MacCtrl+I"
      },
      "description": "Open phone and add inbound tags"
    }
  },
  "web_accessible_resources": [
    "add_tags.js"
  ]
}
